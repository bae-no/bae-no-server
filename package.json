{
  "name": "bae-no-server-new",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "prebuild": "pnpm prisma:generate && rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "console": "nest start --entryFile repl",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/app/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "jest --testPathIgnorePatterns '/integration/.*.spec.ts'",
    "test:integration": "jest --runInBand '/integration/.*.spec.ts'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/bae-no-server-new/test/jest-e2e.json",
    "test:ci": "pnpm test:ci-unit && pnpm test:ci-integration",
    "test:ci-unit": "JEST_JUNIT_OUTPUT_NAME=junit-unit.xml pnpm test:unit --ci --coverage --reporters=default --reporters=jest-junit --coverageDirectory=coverage-unit",
    "test:ci-integration": "JEST_JUNIT_OUTPUT_NAME=junit-integration.xml pnpm test:integration --ci --coverage --reporters=default --reporters=jest-junit --coverageDirectory=coverage-integration",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "graphql:schema": "jest AppModule.spec.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/apollo": "10.2.0",
    "@nestjs/common": "9.3.3",
    "@nestjs/config": "2.3.0",
    "@nestjs/core": "9.3.3",
    "@nestjs/event-emitter": "1.4.1",
    "@nestjs/graphql": "10.2.0",
    "@nestjs/jwt": "10.0.1",
    "@nestjs/passport": "9.0.1",
    "@nestjs/platform-express": "9.3.3",
    "@prisma/client": "4.9.0",
    "apollo-server-core": "3.11.1",
    "apollo-server-express": "3.11.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cuid": "^3.0.0",
    "date-fns": "2.29.3",
    "firebase-admin": "11.5.0",
    "fp-ts": "2.13.1",
    "graphql": "16.6.0",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-subscriptions": "2.0.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.1.2",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.3",
    "@swc-node/jest": "1.5.7",
    "@swc/core": "1.3.32",
    "@swc/helpers": "^0.4.14",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.33.0",
    "eslint-config-async": "2.0.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "express": "4.18.2",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "jest-junit": "15.0.0",
    "jest-mock-extended": "3.0.1",
    "lint-staged": "13.1.1",
    "prettier": "2.8.3",
    "prisma": "4.9.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testTimeout": 30000,
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc-node/jest",
        {
          "swc": {
            "sourceMaps": "inline"
          }
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/domain(|/.*)$": "<rootDir>/libs/domain/src/$1",
      "^@app/pub-sub(|/.*)$": "<rootDir>/libs/pub-sub/src/$1",
      "^@app/http-client(|/.*)$": "<rootDir>/libs/http-client/src/$1",
      "^@app/custom(|/.*)$": "<rootDir>/libs/custom/src/$1",
      "^@app/sms(|/.*)$": "<rootDir>/libs/sms/src/$1",
      "^@app/event-emitter(|/.*)$": "<rootDir>/libs/event-emitter/src/$1",
      "^@app/push-message(|/.*)$": "<rootDir>/libs/push-message/src/$1",
      "^@app/ticket-generator(|/.*)$": "<rootDir>/libs/ticket-generator/src/$1"
    }
  }
}
