{
  "name": "bae-no-server-new",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "prebuild": "pnpm prisma:generate && rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "console": "nest start --entryFile repl",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/app/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "jest --testPathIgnorePatterns '/integration/.*.spec.ts'",
    "test:integration": "jest --runInBand '/integration/.*.spec.ts'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/bae-no-server-new/test/jest-e2e.json",
    "test:ci": "pnpm test:ci-unit && pnpm test:ci-integration",
    "test:ci-unit": "JEST_JUNIT_OUTPUT_NAME=junit-unit.xml pnpm test:unit --ci --reporters=default --reporters=jest-junit",
    "test:ci-integration": "JEST_JUNIT_OUTPUT_NAME=junit-integration.xml pnpm test:integration --ci --reporters=default --reporters=jest-junit",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.17",
    "@nestjs/common": "^9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.3",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.0.0",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.1",
    "fp-ts": "^2.12.1",
    "got": "^11.8.5",
    "graphql": "^16.5.0",
    "graphql-subscriptions": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.3",
    "@swc-node/jest": "^1.5.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "^16.11.45",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-async": "^1.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "jest-junit": "^14.0.0",
    "jest-mock-extended": "^2.0.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testTimeout": 30000,
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc-node/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/domain(|/.*)$": "<rootDir>/libs/domain/src/$1",
      "^@app/pub-sub(|/.*)$": "<rootDir>/libs/pub-sub/src/$1",
      "^@app/http-client(|/.*)$": "<rootDir>/libs/http-client/src/$1",
      "^@app/external(|/.*)$": "<rootDir>/libs/external/src/$1"
    }
  }
}
