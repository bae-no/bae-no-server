  "scripts": {
    "prebuild": "pnpm prisma:generate && rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "OTLP_TRACE=ture nest start --entryFile main",
    "console": "nest start --entryFile repl",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/app/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest run unit",
    "test:integration": "vitest run integration --single-thread",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "test:ci": "pnpm test:ci-unit && pnpm test:ci-integration",
    "test:ci-unit": "pnpm test:unit --coverage.enabled --coverage.reportsDirectory=coverage-unit",
    "test:ci-integration": "pnpm test:integration --coverage.enabled --coverage.reportsDirectory=coverage-integration",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "graphql:schema": "vitest run AppModule.spec.ts",
    "prepare": "husky install"
  },
  "dependencies": {
