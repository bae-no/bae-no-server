{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": {
      "@app/prisma": [
        "libs/prisma/src"
      ],
      "@app/prisma/*": [
        "libs/prisma/src/*"
      ],
      "@app/domain": [
        "libs/domain/src"
      ],
      "@app/domain/*": [
        "libs/domain/src/*"
      ],
      "@app/pub-sub": [
        "libs/pub-sub/src"
      ],
      "@app/pub-sub/*": [
        "libs/pub-sub/src/*"
      ],
      "@app/http-client": [
        "libs/http-client/src"
      ],
      "@app/http-client/*": [
        "libs/http-client/src/*"
      ],
      "@app/custom": [
        "libs/custom/src"
      ],
      "@app/custom/*": [
        "libs/custom/src/*"
      ],
      "@app/sms": [
        "libs/sms/src"
      ],
      "@app/sms/*": [
        "libs/sms/src/*"
      ],
      "@app/event-emitter": [
        "libs/event-emitter/src"
      ],
      "@app/event-emitter/*": [
        "libs/event-emitter/src/*"
      ],
      "@app/push-message": [
        "libs/push-message/src"
      ],
      "@app/push-message/*": [
        "libs/push-message/src/*"
      ],
      "@app/ticket-generator": [
        "libs/ticket-generator/src"
      ],
      "@app/ticket-generator/*": [
        "libs/ticket-generator/src/*"
      ],
      "@app/monitoring": [
        "libs/monitoring/src"
      ],
      "@app/monitoring/*": [
        "libs/monitoring/src/*"
      ]
    }
  }
}
