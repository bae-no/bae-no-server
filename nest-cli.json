{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "monorepo": true,
  "root": "apps/app",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/app/tsconfig.app.json"
  },
  "projects": {
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "domain": {
      "type": "library",
      "root": "libs/domain",
      "entryFile": "index",
      "sourceRoot": "libs/domain/src",
      "compilerOptions": {
        "tsConfigPath": "libs/domain/tsconfig.lib.json"
      }
    },
    "pub-sub": {
      "type": "library",
      "root": "libs/pub-sub",
      "entryFile": "index",
      "sourceRoot": "libs/pub-sub/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pub-sub/tsconfig.lib.json"
      }
    },
    "http-client": {
      "type": "library",
      "root": "libs/http-client",
      "entryFile": "index",
      "sourceRoot": "libs/http-client/src",
      "compilerOptions": {
        "tsConfigPath": "libs/http-client/tsconfig.lib.json"
      }
    },
    "external": {
      "type": "library",
      "root": "libs/external",
      "entryFile": "index",
      "sourceRoot": "libs/external/src",
      "compilerOptions": {
        "tsConfigPath": "libs/external/tsconfig.lib.json"
      }
    }
  }
}